sum = 0
w_i_j = 0
for (k in length(pi_vector)){
# take unif distr if j == 1, else beta distr
if (j == 1){
sum = sum + pi_vector[k] * dunif(x)
}
else {
sum = sum + pi_vector[k] * dbeta(x,alpha,beta)
}
}
# take unif distr if j == 1, else beta distr
if (j == 1) {
w_i_j = (pi_vector[j] * dunif(x)) / sum
}
else {
w_i_j = (pi_vector[j] * dbeta(x, alpha, beta)) / sum
}
sum
}
